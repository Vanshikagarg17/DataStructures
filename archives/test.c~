#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

/* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
	int v,n,i;
	scanf("%d\n",&v);
	scanf("%d",&n);
	int ar[n];
	for(i=0;i<n;i++)
	scanf("%d",&ar[i]);
	for(i=0;i<n;i++)
	{
	if(v==ar[i])
	{
		printf("%d",i);
		break;
	}

	}
return 0;
}
